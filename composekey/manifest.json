{
  "name": "ComposeKey",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Compose Key for Chrome OS",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "input"
  ],
  "input_components": [
    {
      "name": "Compose Key",
      "type": "ime",
      "id": "compose_key",
      "description": "Adds a compose key (RightAlt) functionality to Chrome OS",
      "language": "en-US",
      "layouts": ["us"]
    }
  ]
}
